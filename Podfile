# Uncomment the next line to define a global platform for your project
platform :ios, '15.0'

target 'VPN' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for VPN
  pod 'Firebase/Core', '9.6.0'
  pod 'Firebase/Auth', '9.6.0'
  pod 'GoogleSignIn', '~> 7.0'
  pod 'Alamofire', '~> 5.8'
  pod 'FreshchatSDK'

  # Linting
  pod 'SwiftLint'
end
